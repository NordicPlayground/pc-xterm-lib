{
  "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "outDir": "./lib",
      "target": "ES2019",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "isolatedModules": true,
      "forceConsistentCasingInFileNames": true,
  },
  "include": ["./src/**/*.ts"]
}
